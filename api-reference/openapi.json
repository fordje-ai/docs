  "components": {
    "schemas": {
      "AccessWarning": {
        "properties": {
          "code": {
            "type": "string",
            "title": "Code",
            "description": "Warning code",
            "examples": [
              "AHJ_ACCESS_DENIED"
            ]
          },
          "message": {
            "type": "string",
            "title": "Message",
            "description": "Human-readable explanation"
          },
          "excluded_ahj_ids": {
            "items": {
              "type": "integer"
            },
            "type": "array",
            "title": "Excluded Ahj Ids",
            "description": "AHJ IDs that were excluded from results",
            "examples": [
              [
                45,
                89,
                112
              ]
            ]
          }
        },
        "type": "object",
        "required": [
          "code",
          "message"
        ],
        "title": "AccessWarning",
        "description": "Warning about partial data access due to subscription limits."
      },
      "BaseResponse": {
        "properties": {
          "response_type": {
